{
  "version": 2,
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/auth/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/user/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/tweet/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/comment/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/quote/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/upload/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/message/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/api/v1/conversation/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ]
}
